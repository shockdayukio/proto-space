.container.user-page
  %header.user-nav
    .media
      .media-body
        = content_tag :h4, class: "media-heading", id: "top-aligned-media" do
          Tag List
          %span.label.label-default.project-number
            = @tags.length
            %i Projects
%ul.proto-tag-list.list-inline
  = render partial: "tag", collection: @tags
